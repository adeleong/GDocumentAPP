@model DEPENDENCIA

@{
    ViewBag.Title = "Edit";
}

<h2>Editar Dependencia</h2>
<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title"> Dependencia a Editar</h3>
    </div>
    <div class="panel-body">

        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="form-horizontal">

                @Html.ValidationSummary(true, "", new { @class = "text-danger" })
                @Html.HiddenFor(model => model.DEPENDENCIA_ID)

                <div class="form-group">
                    @Html.LabelFor(model => model.DEPENDENCIA_NOMBRE, htmlAttributes: new { @class = "control-label col-md-2" })
                    <div class="col-md-10">
                        @Html.EditorFor(model => model.DEPENDENCIA_NOMBRE, new { htmlAttributes = new { @class = "form-control" } })
                        @Html.ValidationMessageFor(model => model.DEPENDENCIA_NOMBRE, "", new { @class = "text-danger" })
                    </div>
                </div>
               
            </div>
        }
        </div>
        <div class="panel-footer">
            <div class="input-group col-md-push-1">
                <input type="submit" value="Guardar" class="btn btn-primary" />
                <span> </span>
                @Html.ActionLink("Volver al Listado", "Index", null, new { @class = "btn btn-info" })
            </div>
        </div>
</div>
   

        @section Scripts {
            @Scripts.Render("~/bundles/jqueryval")
        }
