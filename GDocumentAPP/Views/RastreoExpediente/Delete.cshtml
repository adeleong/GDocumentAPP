@model RASTREO_EXPEDIENTE

@{
    ViewBag.Title = "Delete";
}

<!-- Custom CSS -->
<link href="~/Scripts/admin2Template/dist/css/sb-admin-2.css" rel="stylesheet" />

<h2>Eliminar Expediente Fisico</h2>

<div class="panel panel-default">
    <div class="panel-heading">
        <h3 class="panel-title">¿Estás seguro que desea eliminar este Registro de Expediente Fisico?</h3>
    </div>
    <div class="panel-body">
        <dl class="dl-horizontal">
            <dt>
                Empleado
            </dt>

            <dd>
                @{ var EmpleadoDisplay = Model.EMPLEADO.getEmpleadoDisplay();}
                @EmpleadoDisplay
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.FECHA_SALIDA)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.FECHA_SALIDA)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.FECHA_DEVOLUCION)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.FECHA_DEVOLUCION)
            </dd>

            <!--
            <dt>
                @Html.DisplayNameFor(model => model.FIRMA)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.FIRMA)
            </dd> -->

            <dt>
                @Html.DisplayNameFor(model => model.ESTATUS_ID)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.ESTATU.DESCRIPCION)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.PERSONA_ID)
            </dt>

            <dd>
                @{ var personaDisplay = Model.PERSONA.getPersonaDisplayFull();}
                @personaDisplay
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.COMENTARIO)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.COMENTARIO)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.DEPENDENCIA.DEPENDENCIA_NOMBRE)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.DEPENDENCIA.DEPENDENCIA_NOMBRE)
            </dd>

            <dt>
                @Html.DisplayNameFor(model => model.USUARIO.LOGIN)
            </dt>

            <dd>
                @Html.DisplayFor(model => model.USUARIO.LOGIN)
            </dd>

        </dl>
    </div>

    <div class="panel-footer">
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

            <div class="input-group col-md-push-1">
                <input type="submit" value="Eliminar" class="btn btn-danger" />
                <span> </span>
                @Html.ActionLink("Volver al Listado", "Index", null, new { @class = "btn btn-info" })
            </div>
        }
    </div>
</div>